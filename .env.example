# Alchemy API key (optional, for additional RPC data)
ETH_ALCHEMY_API_KEY=

# FRED API key for US Treasury rates (optional)
# Get your key at: https://fred.stlouisfed.org/docs/api/api_key.html
# If not set, uses fallback T-bill rate of 5%
FRED_API_KEY=

# Aave v3 API URL (optional, default uses official free API)
# AAVE_API_URL=https://api.v3.aave.com/graphql

# Cache configuration
CACHE_DIR=.cache/morpho
CACHE_TTL_SECONDS=300

# UI settings
UI_REFRESH_INTERVAL=60

# Wallet addresses to track (comma-separated)
WALLET_ADDRESSES=
